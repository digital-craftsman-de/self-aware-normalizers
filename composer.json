{
  "name": "digital-craftsman/self-aware-normalizers",
  "description": "Adds interfaces and normalizers that allow value objects to normalize and denormalize themselves.",
  "type": "symfony-bundle",
  "require": {
    "php": "8.3.*|8.4.*",
    "symfony/framework-bundle": "^7.1",
    "symfony/serializer": "^7.1",
    "doctrine/dbal": "^3.3.6",
    "league/construct-finder": "^1.5",
    "symfony/yaml": "^7.1"
  },
  "require-dev": {
    "vimeo/psalm": "^6.0.0",
    "friendsofphp/php-cs-fixer": "^v3.68.5",
    "phpunit/phpunit": "^12.4",
    "infection/infection": "^0.31.8",
    "symfony/property-access": "^7.1",
    "symfony/property-info": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "DigitalCraftsman\\SelfAwareNormalizers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DigitalCraftsman\\SelfAwareNormalizers\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Kolb",
      "email": "info@digital-craftsman.de"
    }
  ],
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
